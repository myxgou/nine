@import "/utils/css/common.wxss";



.sucess-board-view {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.notice-image {
  margin-top: 30px;
  width: 132px;
  height: 132px;
}


.notice-title {
  width: 100%;
  margin-top: 27px;
  font-size: 16px;
  height: 18px;
  line-height: 18px;
  text-align: center;
  color: #2AA515;
}
.notice-tips {
  margin-top: 16px;
  width: 100%;
  height: 14px;
  line-height: 14px;
  font-size: 14px;
  text-align: center;
  color: #ACACB2;
}

.submit-button {
  width: 90%;
  margin-left: 5%;
  height: 45px;
  margin-top: 50px;
}
