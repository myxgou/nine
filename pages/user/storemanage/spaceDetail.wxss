.header-board-view {
  width: 100%;
  height: 120px;
  display: flex;
  flex-direction: row;
}

.detail-icon-board {
  margin: 10px;
  width: 100px;
  flex: none;
}

.detail-info-board {
  flex: 1;
}

.space-icon {
  width: 100px;
  height: 100px;
  margin-top: 10px;
}

.add-item-board {
  position: fixed;
  bottom: 0;
  left:0;
  height: 50px;
  text-align: center;
  width: 100%
}

.space-item-add {
  width: 50px;
  height: 50px;
}

.space-item-board-view {
  display: flex;
  flex-flow: row wrap;
  width: 100%;
}

.space-item-view {
  width: 25%;
  height: 60px;
}

.space-item-image {
  width: 40px;
  height: 40px;
}

.space-item-title {
  height: 20px;
  color: #888888;
  font-size: 12px;
}