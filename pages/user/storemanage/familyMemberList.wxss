.member-add-board {
  text-align: center;
  height: 60px;
  position: fixed;
  bottom: 0px;
  left: 0px;
  width: 100%;
}

.member-add {
  width: 60px;
  height: 60px;
}

.member-item-board-view {
  width: 50%;
  height: 100px;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.member-item-board-view image {
  width: 80px;
  height: 80px;
}

.selected-image {
  width: 10px;
  height: 10px;
  position: relative;
  right: 5px;
  bottom: 5px;
}


